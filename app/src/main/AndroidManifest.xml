<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.todoapp" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TuhYenApp"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".HistoryActivity" />
        <activity android:name=".Home" />
        <activity android:name=".InTuhYen" />
        <activity android:name=".skin_popup" />
        <activity android:name=".ToDoList" />
        <activity android:name=".TaskActivity" />
        <activity android:name=".Home_skin_black" />
        <activity android:name=".Home_skin_blue" />
        <activity android:name=".InTuhYen_skin_black" />
        <activity android:name=".InTuhYen_skin_blue" />
        <activity android:name=".splash" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>